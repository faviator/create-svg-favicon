  "bugs": {
    "url": "https://github.com/faviator/create-svg-favicon/issues"
  },
  "homepage": "https://github.com/faviator/create-svg-favicon#readme",
  "devDependencies": {
    "@ycm.jason/eslint-config": "^1.0.0",
    "codecov": "^3.0.0",
    "eslint": "^4.17.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1"
  }
}
